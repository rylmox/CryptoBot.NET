{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug",
        "type": "dotnet",
        "request": "launch",
        "projectPath": "${workspaceFolder}/Console/Console.csproj",
        "internalConsoleOptions": "openOnSessionStart",
      },
      {
        "name": "Launch Console",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/Console/bin/Debug/net9.0/Console.dll",
        "args": ["--config=appsettings.json"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "stopAtEntry": false
    }
    ]
  }
  